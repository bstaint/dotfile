URxvt.colorBD:white  
URxvt.colorUL:Green  
URxvt.background: black  
URxvt.foreground: white 
URxvt.scrollBar: false
Xft.dpi: 96
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb
! URxvt.allow_bold: True
!URxvt*font: xft:Source Code Pro:size=12, xft:Source Han Sans CN:size=11, xft:DejaVu Sans Mono:size=11
URxvt.font:     xft:bitstream vera sans mono:size=11:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=11:antialias=true
URxvt*letterSpace: -2
URxvt.cursorBlink:True
URxvt.inheritPixmap: True  
URxvt.tintColor: grey  
! URxvt.shading: 80 
URxvt.matcher.button: 1  
! URxvt.perl-ext-common: default,tabbed  
! URxvt.tabbed.tab-fg: 10  
! URxvt.tabbed.tab-bg: 0  
! URxvt.tabbed.tabbar-fg: 5 
URxvt.geometry: 80x24
! URxvt.transparent:true

!Black  
URxvt.color0:  #000000  
URxvt.color8:  #555753  
!Red  
URxvt.color1:  #CC0000  
URxvt.color9:  #EF2929  
!Green  
URxvt.color2:  #4E9A06  
URxvt.color10: #8AE234  
!Yellow  
URxvt.color3:  #C4A000  
URxvt.color11: #FCE94F  
!Blue  
URxvt.color4:  #3465A4  
URxvt.color12: #729FCF  
!Magenta  
URxvt.color5:  #75507B  
URxvt.color13: #AD7FA8  
!Cyan  
URxvt.color6:  #06989A  
URxvt.color14: #34E2E2  
!White  
URxvt.color7:  #D3D7CF  
URxvt.color15: #EEEEEC  
